                    <div class="tab-pane fade" id="activities" role="tabpanel">
                        <div class="card">
                            <div class="card-body px-0">
                                <h5 class="card-title mb-4 px-3">Mis movimientos</h5>
                                <div class="timeline mt-5" data-simplebar style="max-height: 405px;" class="p-3 pt-0">
                                    {% if request.session.persona.mismovimientos %}
                                        {% for movimiento in request.session.persona.mismovimientos %}
                                            <div class="timeline-line">
                                                <div class="timeline-box ">
                                                    <div class="date">{{ movimiento.fech_creacion|date:'Y-m-d'}}</div>
                                                    <div class="content">
                                                        <h5>{% if movimiento.compra%} Compra
                                                            {% if movimiento.compra.estadocompra == 1 %}
                                                                <span class="badge badge-soft-info">Pendiente</span>
                                                            {% elif list.estadocompra == 2 %}
                                                                <span class="badge badge-soft-danger">Rechazado</span>
                                                            {% else %}
                                                                <span class="badge badge-soft-success">Aprobado</span>
                                                            {% endif %}
                                                        {% else %}
                                                            Depósito
                                                            {% if movimiento.deposito.estadodeposito == 1 %}
                                                                <span class="badge badge-soft-info">Pendiente</span>
                                                            {% elif list.estadodeposito == 2 %}
                                                                <span class="badge badge-soft-danger">Rechazado</span>
                                                            {% else %}
                                                                <span class="badge badge-soft-success">Aprobado</span>
                                                            {% endif %}
                                                        {% endif %}</h5>
                                                        <p class="text-muted mb-0">
                                                            {% if movimiento.compra %}
                                                                <img src="{% if movimiento.compra.detalleproducto.imagen %} {{ movimiento.compra.detalleproducto.imagen.url}} {% else %} {% static 'assets/img/sin_imagen.jpeg' %} {% endif %} " alt="" class="img-thumbnail" width="40px">
                                                                {{ movimiento.compra.detalleproducto.nombre}} - {{ movimiento.compra.detalleproducto.descripcion}}
                                                            {% else %}
                                                                <img src="{% if movimiento.deposito.cuenta.banco.icono %} {{ movimiento.deposito.cuenta.banco.icono.url}} {% else %} {% static 'assets/img/sin_imagen.jpeg' %} {% endif %} " alt="" class="img-thumbnail" width="40px">
                                                                {{ movimiento.deposito.cuenta.banco.nombre}}<br>
                                                                ${{ movimiento.deposito.valor}}
                                                            {% endif %}
                                                            <br>
                                                            <span class="badge badge-soft-info">Hace, {{ movimiento.fech_creacion|timesince }}</span>
                                                        </p>
                                                    </div>
                                                </div>
                                            </div>
                                        {% endfor %}
                                    {% else %}
                                        <div class="timeline-line">
                                            <div class="timeline-box ">
                                                <div class="date">Si detalle</div>
                                                <div class="content">
                                                    <h5>Sin movimientos</h5>
                                                    <p class="text-muted mb-0">
                                                        No se han registrado aún movimientos desde esta cuenta
                                                    </p>
                                                </div>
                                            </div>
                                        </div>
                                    {% endif %}

                                </div>

                            </div>
                        </div>
